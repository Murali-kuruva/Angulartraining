
    <h1>List of Books</h1>
    <a class='btn btn-primary' href='/books/create'>create Book</a>
    <table class='table table-condensed'>
        <thead>
            <tr>
                <th>title</th>
                <th>coverPageUrl</th>
                <th>author</th>
                <th>price</th>
            </tr>
        </thead>

        <tbody>
            <% for(let book of books){  %>
            <tr>
                <td><%= books.name %></td>
                <td><%= books.coverPageUrl></td>
                <td><%= books.author></td>
                <td><%= books.price></td>
                <td>
                    <a class='btn btn-sm btn-primary' href='/authors/details/<%=books.id%>'>Details</a>
                    <a class='btn btn-sm btn-primary' href='/authors/edit/<%=author.id%>'>Edit</a>
                    <a class='btn btn-sm btn-danger' href='/authors/delete/<%=books.id%>'>Delete</a>
                </td>
            </tr>
            <%}%>

        </tbody>

    </table>
